{
  "name": "api-automation",
  "version": "0.0.1",
  "description": "A project boilerplate for testing APIs using TypeScript and Jest.",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./src --ext .js",
    "lint:fix": "eslint --fix ./src --ext .js",
    "test": "NODE_ENV=dev SM=uk jest",
    "test:test": "NODE_ENV=preprod SM=uk jest",
    "test:smoke": "NODE_ENV=dev SM=uk jest -t @smoke",
    "test:watch": "yarn test --watch"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "23.8.2",
    "jest": "25.2.7",
    "jest-html-reporter": "3.0.0",
    "jest-json-reporter": "1.2.2",
    "prettier": "2.0.4",
    "rimraf": "3.0.2",
    "tsc-watch": "4.2.3",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/register": "^7.14.5",
    "api-automation": "/Users/murillowelsi/repos/api-automation",
    "axios": "0.19.2",
    "cd": "^0.3.3",
    "dotenv": "^10.0.0",
    "eslint-plugin-eslint-plugin": "^3.1.0"
  }
